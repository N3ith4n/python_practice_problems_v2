#makes a function like the lower() function
def custom_lower(input_string):
    lowercase_string = ""
    
    for character in input_string:
        if 'A' <= character <= 'Z':  # Check if the character is uppercase
            # Convert to lowercase by adding 32 to the ASCII value
            lowercase_character = chr(ord(character) + 32)
            lowercase_string += lowercase_character
        else:
            lowercase_string += character  # Keep non-uppercase characters unchanged
            
    return lowercase_string

